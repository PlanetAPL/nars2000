        MENUITEM "&Copy\tCtrl-Ins",             IDM_COPY
        POPUP "Cop&y to ..."
        BEGIN
            MENUITEM "APL+&Win",                IDM_COPY_APLWIN
            MENUITEM "APL&2",                   IDM_COPY_APL2
            MENUITEM "&Dyalog",                 IDM_COPY_DYALOG
            MENUITEM "&ISO",                    IDM_COPY_ISO
