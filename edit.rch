            MENUITEM "&Braces"                  IDM_COPY_BRACES
        END
        MENUITEM "&Paste\tShift-Ins",           IDM_PASTE
        POPUP "Past&e from ..."
        BEGIN
            MENUITEM "APL+&Win",                IDM_PASTE_APLWIN
            MENUITEM "APL&2",                   IDM_PASTE_APL2
            MENUITEM "&Dyalog",                 IDM_PASTE_DYALOG
            MENUITEM "&ISO",                    IDM_PASTE_ISO
            MENUITEM "&PC3270"                  IDM_PASTE_PC3270
            MENUITEM "&Braces"                  IDM_PASTE_BRACES
        END
        MENUITEM "&Delete",                     IDM_DELETE
        MENUITEM SEPARATOR
        MENUITEM "Select &All",                 IDM_SELECTALL
        MENUITEM SEPARATOR
        MENUITEM "Customi&ze...",               IDM_CUSTOMIZE


